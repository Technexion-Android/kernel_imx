 /dts-v1/;

#include "imx8mm-pico-pi.dts"
#include "imx8mm-android.dtsi"

/ {
	model = "[ANDROID] TechNexion PICO-IMX8MM with PI baseboard";

    bt_rfkill {
		compatible = "fsl,mxc_bt_rfkill";
		pinctrl-0 = <&pinctrl_bt_ctrl>;
		bt-power-gpios = <&gpio1 3 GPIO_ACTIVE_LOW>;
		status ="okay";
	};
};

&vpu_v4l2 {
	status = "okay";
};
